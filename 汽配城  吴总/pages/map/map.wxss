page {
    height: 100%;
}

.container {
    height: 100%;
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
    flex-wrap: wrap;
}

.button {
    height: 80rpx;
    padding: 0 15rpx;
    margin: 15rpx auto;
    background: #000;
    line-height: 80rpx;
    color: #fff;
    border-radius: 15rpx;
}

.button:active {
    background: #0261ef;
}

map {
    width: 100%;
    height: calc(100% - 110rpx);
}
